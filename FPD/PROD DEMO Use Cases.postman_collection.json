				{
					"name": "get ATH token",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									""
								],
								"type": "text/javascript"
							}
						}
					],
					"request": {
						"auth": {
							"type": "noauth"
						},
						"method": "POST",
						"header": [
							{
								"key": "Content-Type",
								"value": "application/x-www-form-urlencoded"
							},
							{
								"key": "Authorization",
								"value": "Basic {{client}}"
							},
							{
								"key": "grant_type",
								"value": "client_credentials"
							}
						],
						"body": {
							"mode": "urlencoded",
							"urlencoded": [
								{
									"key": "grant_type: client_credentials",
									"value": "",
									"type": "text"
								}
							]
						},
						"url": {
							"raw": "{{TOKEN_SERVICE}}/v1/auth/token",
							"host": [
								"{{TOKEN_SERVICE}}"
							],
							"path": [
								"v1",
								"auth",
								"token"
							]
						}
					},
					"response": []
				}
			],
			"event": [
				{
					"listen": "prerequest",
					"script": {
						"type": "text/javascript",
						"exec": [
							"const username = pm.collectionVariables.get('username');",
							"const pcc = pm.collectionVariables.get('pcc');",
							"const passwordRaw = pm.collectionVariables.get('password');",
							"",
							"const clientidRaw = `V1:${username}:${pcc}:AA`;",
							"const clientidArray = CryptoJS.enc.Utf8.parse(clientidRaw);",
							"const clientidBase64 = CryptoJS.enc.Base64.stringify(clientidArray);",
							"const passwordArray = CryptoJS.enc.Utf8.parse(passwordRaw);",
							"const passwordBase64 = CryptoJS.enc.Base64.stringify(passwordArray);",
							"const secretRaw = `${clientidBase64}:${passwordBase64}`;",
							"const secretArray = CryptoJS.enc.Utf8.parse(secretRaw);",
							"const secretBase64 = CryptoJS.enc.Base64.stringify(secretArray);",
							"",
							"pm.collectionVariables.set('client', secretBase64);",
							"pm.collectionVariables.set('token', \"\");",
							""
						]
					}
				},
				{
					"listen": "test",
					"script": {
						"type": "text/javascript",
						"exec": [
							"const jsonData = JSON.parse(responseBody);",
							"pm.collectionVariables.set('token', jsonData.access_token);"
						]
					}
				}
			]
		},
